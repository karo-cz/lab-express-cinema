<div class="movies">
  {{#each movieList}}
  <div class="movie-card">
    <div class="image">
      <img src="{{image}}" alt="">
    </div>
    <div class="title">
      <h3>{{title}}</h3>
    </div>
    <div class="link">
      <a href="/movies/{{_id}}">See more</a>
    </div>
  </div>
  {{/each}}

</div>